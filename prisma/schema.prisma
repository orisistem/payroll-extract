generator client {
    provider             = "prisma-client-py"
    interface            = "asyncio"
    recursive_type_depth = 5
}

datasource db {
    provider = "sqlite"
    url      = "file:./data.db"
}

model Payroll {
    id          String   @id @default(uuid())
    employee    String
    position    String
    gross_value Float
    net_value   Float
    month_year  String
    createdAt   DateTime @default(now())
}
